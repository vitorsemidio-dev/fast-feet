{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.organizeImports": true
  },
  "eslint.format.enable": true,
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "material-icon-theme.activeIconPack": "nest",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#705cd6",
    "activityBar.background": "#705cd6",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#66261a",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#705cd6",
    "statusBar.background": "#4c33cc",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#705cd6",
    "statusBarItem.remoteBackground": "#4c33cc",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#4c33cc",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#4c33cc99",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "4C33CC"
}
